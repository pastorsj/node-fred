{
    "name": "node-fred",
    "version": "3.0.0",
    "description": "Another nodejs wrapper for the st louis fed",
    "type": "module",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        }
    },
    "engines": {
        "node": ">=18"
    },
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "lint": "eslint src/* tests/*",
        "autofix": "eslint --fix src/* tests/*",
        "test": "mocha --timeout 5000 ./tests/**/*.test.js",
        "test:local": "node --env-file=.env node_modules/mocha/bin/mocha.js --timeout 5000 ./tests/**/*.test.js",
        "test:watch": "mocha --timeout 5000 -w ./tests/**/*.test.js",
        "cover": "nyc --reporter=text --reporter=html mocha --timeout 5000 --reporter dot ./tests/**/*.test.js",
        "cover:lcov": "nyc --reporter=lcovonly mocha --timeout 5000 --reporter spec ./tests/**/*.test.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/pastorsj/node-fred.git"
    },
    "keywords": [
        "fred2",
        "fred",
        "nodejs"
    ],
    "author": "pastorsj",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/pastorsj/node-fred/issues"
    },
    "homepage": "https://github.com/pastorsj/node-fred#readme",
    "dependencies": {
        "axios": "^1.13.4"
    },
    "devDependencies": {
        "chai": "^5.3.3",
        "eslint": "^8.57.1",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-plugin-import": "^2.32.0",
        "mocha": "^11.3.0",
        "nyc": "^17.1.0",
        "sinon": "^21.0.1",
        "sinon-chai": "^4.0.1",
        "tsup": "^8.5.1",
        "typescript": "^5.9.3"
    }
}
